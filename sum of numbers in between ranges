class HelloWorld {
    public static void main(String[] args) {
        int n1 = 5;
        int n2 = 10; 
        int s=0;
        for(int i=n1;i<=n2;i++){
            s+=i;
        }
        System.out.println(s);
    }
}


by using formula
class HelloWorld {
    public static void main(String[] args) {
        int n1 = 5;
        int n2 = 10; 
        int s=n2*(n2+1)/2 - n1*(n1+1)/2 + n1;
        System.out.println(s);
    }
}
